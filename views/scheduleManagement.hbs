<form action='/schedules/new' method='POST'>
    <select name='user_id'>
        {{#each userData}}
            <option value={{this.id}}>{{this.first_name}}
            </option>
        {{/each}}

    </select>

    <br /><br />
    <input type='text' name='start_time' placeholder='Start YYYY-MM-DD HH:MM' />
    <br /><br />
    <input type='text' name='end_time' placeholder='End YYYY-MM-DD HH:MM' />
    <br /><br />
    <input type='submit' value='Submit' />
</form>